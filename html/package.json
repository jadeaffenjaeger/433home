{
    "name": "433home",
    "version": "1.0.0",
    "description": "Minimal Server to control 433MHz power sockets",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "source": [
        "./src/index.html",
        "./src/uptime.html",
        "./src/ip.html",
        "./src/rssi.html",
        "./src/ssid.html",
