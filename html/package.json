    },
    "dependencies": {
        "@fontsource/lexend-peta": "^5.0.8",
        "bootstrap": "^5.3.2",
        "htmx.org": "^1.9.5",
        "parcel": "^2.9.3"
    },
    "devDependencies": {
        "@parcel/compressor-gzip": "^2.9.3",
        "@parcel/transformer-sass": "^2.9.3"
    }
}
